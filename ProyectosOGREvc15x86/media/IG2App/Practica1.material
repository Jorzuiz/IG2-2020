material IG2App/water {
    technique {
        pass {
            diffuse 0.5 0.5 0.5
            texture_unit {
                texture BeachStones.jpg
                colour_op add
            }
            texture_unit {
                scroll_anim -0.1 0.0
                texture Water02.jpg
                colour_op modulate
            }
        }
    }
}
