material IG2App/Panel	{
	technique {
		pass {
			lighting off
			texture_unit {
				texture spotlight_image.png
				tex_address_mode clamp
			}
		}
	}
}
material IG2App/Panel1	{
	technique {
		pass {
			lighting off
			texture_unit {
				texture BumpyMetal.jpg
				tex_address_mode clamp
			}
		}
	}
}

material IG2App/Smoke {
	technique {
		pass {
			lighting off
			scene_blend alpha_blend
			depth_write off
			diffuse vertexcolour
			texture_unit {
					texture smoke.png
					tex_address_mode clamp
			}
		}	
	}
}
material IG2App/space {
	technique {
        	pass {
            		diffuse 0.5 0.5 0.5
            		texture_unit {
				rotate_anim 0.25
                		texture numbers.jpg
                		colour_op add
            		}
            		texture_unit {                
                		texture lightMap.jpg
                		colour_op modulate
            		}
        	}
    	}
}